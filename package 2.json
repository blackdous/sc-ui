{
  "name": "sc-ui",
  "version": "0.0.0",
  "description": "vue3 ui ant-design-vue",
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/sc-ui.umd.cjs",
  "module": "./dist/sc-ui.js",
  "exports": {
    ".": {
      "import": "./dist/sc-ui.js",
      "require": "./dist/sc-ui.umd.cjs"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "initial-scan docs && vitepress dev .docs --host",
    "build": "vite build",
    "site:build": "cross-env NODE_ENV=production vitepress build .docs",
    "serve": "vitepress serve .docs --host",
    "deploy": "gh-pages -d .docs/.vitepress/dist -t true",
    "docs-build-deploy": "yarn docs-build && yarn docs-deploy",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "lint": "eslint src/**/*.{vue,js,ts,tsx}",
    "lint-fix": "yarn lint -- --fix",
    "release": "np"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@ant-design/colors": "^6.0.0",
    "@ant-design/icons-vue": "^6.1.0",
    "@pnpm/types": "^8.8.0",
    "@ruabick/md-demo-plugins": "0.2.8",
    "@ruabick/vite-plugin-gen-api-doc": "^0.2.8",
    "@ruabick/vite-plugin-gen-temp": "^0.2.8",
    "@ruabick/vitepress-demo-block": "^0.2.8",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "@vitejs/plugin-legacy": "^2.3.0",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vueuse/core": "^9.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.20.0",
    "eslint-define-config": "^1.6.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "gh-pages": "^4.0.0",
    "less": "^4.1.3",
    "less-vars-to-js": "^1.3.0",
    "np": "^7.6.2",
    "postcss": "^8.4.18",
    "postcss-less": "^6.0.0",
    "prettier": "^2.7.1",
    "rollup-plugin-visualizer": "^5.8.3",
    "unplugin-vue-components": "^0.22.9",
    "unplugin-vue-define-options": "^0.12.6",
    "vite": "^3.2.2",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-dts": "^1.4.0",
    "vite-plugin-style-import": "^2.0.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-theme": "^0.8.6",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vitepress": "1.0.0-alpha.26",
    "vue": "3.2.25",
    "vue-eslint-parser": "^9.1.0",
    "vue-tsc": "^1.0.9"
  },
  "dependencies": {
    "@iconify/vue": "^4.0.0",
    "ant-design-vue": "2.2.8"
  }
}
