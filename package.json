    "vitest": "^0.24.5",
    "vue": "3.2.25",
    "vue-eslint-parser": "^9.1.0",
    "vue-tsc": "^1.0.9",
    "vue-types": "^4.2.1",
    "windicss": "^3.5.6"
  },
  "dependencies": {
    "@ant-design/icons-svg": "^4.2.1",
    "@iconify/vue": "^4.0.0",
    "@sincerecloud/sc-ui": "^0.0.73",
    "@vue/shared": "^3.2.47",
    "ant-design-vue": "2.2.8",
