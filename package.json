    "resize-observer-polyfill": "^1.5.1",
    "vue-slider-component": "4.1.0-beta.7"
  },
  "husky": {
    "pre-commit": "yarn build && yarn site:build"
  },
  "engines": {
    "node": ">= 16.14"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "vite",
        "react",
        "react-dom"
      ]
    }
  },
  "lint-staged": {
    "*.{vue,js,ts,jsx,tsx,md,json}": "eslint --fix"
  }
}